<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bdir.masterhr.domain.Clocking" table="Clocking">
        <id name="idClocking" column="idClocking"/>
        <property name="idTimesheet" column="idTimesheet"/>
        <property name="usernameEmployee" column="usernameEmployee"/>
        <property name="fromHour" type="org.hibernate.type.LocalDateTimeType" column="fromHour"/>
        <property name="toHour" type="org.hibernate.type.LocalDateTimeType" column="toHour"/>
        <property name="reason" column="reason"/>
    </class>
</hibernate-mapping>